{
    "name": "vincentlanglet/symfony-custom-coding-standard",
    "type": "coding-standard",
    "description": "Customized CodeSniffer rule set for the Symfony projects",
    "keywords": [
        "Symfony",
        "coding standard",
        "phpcs",
        "twigcs"
    ],
    "homepage": "https://github.com/VincentLanglet/Symfony-custom-coding-standard",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Langlet"
        }
    ],
    "bin": ["bin/twigcs"],
    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/console": "^4.4 || ^5.0",
        "symfony/twig-bridge": "^4.4 || ^5.0",
        "twig/twig": "^2.12 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "TwigCS\\": "TwigCS/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TwigCS\\Tests\\": "TwigCS/tests/"
        }
    }
}
